<shapes>
<!-- begin adaptation states model -->
<shape name="transition" aspect="variable">
	<background>
		<path>
			<move x="0" y="0"/> 
			<line x="90" y="0"/> 
			<line x="100" y="50"/> 
			<line x="90" y="100"/> 
			<line x="0" y="100"/> 
			<line x="10" y="50"/> 
			<line x="0" y="0"/> 
			<close/>
		</path>		
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>
</shapes>