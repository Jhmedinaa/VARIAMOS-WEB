<shapes>

<shape name="board" aspect="variable">
	<background>
		<path>
			<move x="25" y="00"/>
			<line x="100" y="00"/>  
			<line x="100" y="100"/> 
			<line x="0" y="100"/> 
			<line x="0" y="25"/> 
			<line x="25" y="0"/> 
			<close/>  
		</path>	
	</background>
	<foreground>
		<fillstroke/>
	</foreground>
</shape>

