	<foreground> 
		<image src="./images/models/adaptation_hardware/sensor.png" x="1" y="3" w="15" h="35" />
		<image src="./images/models/adaptation_hardware/analog.png" x="84" y="3" w="15" h="35" />	
	</foreground>
</shape>

